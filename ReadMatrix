package javalab;
import java.util.Scanner;
public class ReadMatrix 
{
	public static void main(String args[])
	{
    Scanner sc=new Scanner(System.in);
    int A[][]=new int[5][5];
	for(int i=0;i<3;i++)
	{
		for(int j=0;j<2;j++)
		{   
			System.out.print("A["+i+"]["+j+"]=");
		    A[i][j]=sc.nextInt();
			
		}
	}
	for(int i=0;i<3;i++)
	{
		for(int j=0;j<2;j++)
		{  
			System.out.print(A[i][j] +"\t");
		}
		System.out.println();
		
	}
	}
}
